<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ShadowStyle">
        <item name="android:shadowColor">#6e6e6e</item>
        <item name="android:shadowDx">0.5</item>
        <item name="android:shadowDy">0.5</item>
        <item name="android:shadowRadius">0.5</item>
    </style>

    <style name="DeepStyle">
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">-0.5</item>
        <item name="android:shadowDy">-0.5</item>
        <item name="android:shadowRadius">0.5</item>
    </style>
    <style name="OrangeStyle">
        <item name="android:shadowColor">#742061</item>
        <item name="android:shadowDx">-0.5</item>
        <item name="android:shadowDy">-0.5</item>
        <item name="android:shadowRadius">0.5</item>
    </style>

    <style name="Dialog_Fullscreen">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="GreenStyle">
        <item name="android:shadowColor">#003300</item>
        <item name="android:shadowDx">-0.5</item>
        <item name="android:shadowDy">-0.5</item>
        <item name="android:shadowRadius">0.5</item>
    </style>

    <style name="HomeButtonStytle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20dip</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">10dip</item>
    </style>

    <style name="FileViewToolbarButtonStytle">
        <item name="android:padding">10dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16dip</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FileViewOperationButtonStytle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14dip</item>
        <item name="android:textColor">#666666</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/operation_button_background</item>
    </style>

    <style name="FileViewOperationMoveButtonStytle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18dip</item>
        <item name="android:textColor">#666666</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">5dip</item>
        <item name="android:layout_marginRight">5dip</item>
        <item name="android:background">@drawable/remote_button_bg</item>
    </style>

    <style name="CategoryButtonStyle">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="CategoryButtonIconStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginBottom">3px</item>
    </style>
    <style name="CategoryButtonTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff323232</item>
    </style>
    <style name="CategoryButtonCountTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#ff9d9d9e</item>
    </style>

    <style name="InformationItemTitleStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">60dip</item>
        <item name="android:gravity">right</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">14dip</item>
    </style>

    <style name="InformationItemContentStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">left</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">14dip</item>
        <item name="android:layout_marginLeft">5dip</item>
    </style>
    <style name="InformationItemStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:paddingLeft">5dip</item>
        <item name="android:layout_marginBottom">5dip</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="InformationTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">#222222</item>
        <item name="android:textSize">12dip</item>
        <item name="android:drawablePadding">5dip</item>
    </style>
    <style name="CategoryLegendStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">left</item>
        <item name="android:drawablePadding">5px</item>
        <item name="android:textColor">#222222</item>
        <item name="android:textSize">10dip</item>
        <item name="android:layout_marginLeft">2dip</item>
    </style>
    <style name="CategoryLegendStyleLand">
        <item name="android:layout_height">0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">left</item>
        <item name="android:drawablePadding">5px</item>
        <item name="android:textColor">#222222</item>
        <item name="android:textSize">10dip</item>
        <item name="android:layout_marginLeft">2dip</item>
    </style>
    <style name="InformationDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/popup_bg</item>
    </style>
    <style name="ActionButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">@style/TextAppearance.ActionButton</item>
    </style>

    <style name="TextAppearance" parent="android:TextAppearance">
    </style>

    <style name="TextAppearance.ActionButton">
        <item name="android:textStyle">italic</item>
    </style>

    <style name="BottomButton" >
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
        <item name="android:textColor">#cc000000</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:shadowColor">#80ffffff</item>
        <item name="android:shadowRadius">0.2</item>
        <item name="android:shadowDy">1</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/btn_bottom</item>
        <item name="android:textSize">17sp</item>		
    </style>
	
	<style name="BottomButton.Left" parent="@style/BottomButton" />
	<style name="BottomButton.Right" parent="@style/BottomButton" />
	
    <style name="BottomButtonBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">17sp</item>
        <item name="android:background">@drawable/bottom_button_bar</item>
    </style>
    
    
    
    
    
    <style name="MMTabButton">
        <item name="android:textSize">12.0dip</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1.0</item>
    </style>    
    <style name="ActionBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/actionbar_color</item>
        <item name="android:displayOptions">showTitle</item>
    </style>    
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <item name="android:checkBoxPreferenceStyle">@style/Freeme_CheckBoxPreference</item>
        <item name="android:actionBarStyle">@style/ActionBarStyle</item>
        <item name="android:actionModeBackground">@color/transparent</item>
        <item name="android:actionModeCloseDrawable">@drawable/menu_item_back</item>
        <item name="android:actionOverflowButtonStyle">@style/MyOverflowStyle</item>
        <item name="android:homeAsUpIndicator">@drawable/menu_item_back</item>
    </style>    
    <style name="MyOverflowStyle" parent="@android:style/Widget.Holo.Light.ActionButton.Overflow">
        <item name="android:src">@drawable/fm_overflow</item>
    </style>    
    <style name="Freeme_CheckBoxPreference">
        <item name="android:widgetLayout">@layout/preference_widget_checkbox</item>
    </style>  
    <style name="CategoryButtonStylesmall">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">@dimen/CategoryButtonStylesmall_layout_height</item>
        <item name="android:layout_marginLeft">40.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>    
    <style name="CategoryButtonStyleTextsmall">
        <item name="android:gravity">center|left</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">100.0dip</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>   
    <style name="CategoryButtonIconStylesmall">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="CategoryButtonDocStyleTextsmall">
        <item name="android:gravity">center|left</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">100.0dip</item>
        <item name="android:layout_marginLeft">10.0dip</item>
    </style>    
    <style name="CategoryButtonDocStylesmall">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/CategoryButtonStylesmall_layout_height</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="SafeBoxButtonStyle">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@drawable/v3_button_bg</item>
        <item name="android:layout_width">270.0dip</item>
        <item name="android:layout_height">50.0dip</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="SafeBoxButtonStyle2">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@drawable/v3_button_bg</item>
        <item name="android:layout_width">150.0dip</item>
        <item name="android:layout_height">50.0dip</item>
    </style>
    <style name="safeboxFormContainer">
        <item name="android:padding">16.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="safebox_edit_content">
        <item name="android:textSize">18.0sp</item>
        <item name="android:paddingTop">12.0dip</item>
        <item name="android:paddingBottom">12.0dip</item>
        <item name="android:paddingStart">4.0dip</item>
        <item name="android:layout_marginStart">4.0dip</item>
    </style>              
</resources>
